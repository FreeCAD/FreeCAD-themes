/* QScrollBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qscrollbar

--------------------------------------------------------------------------- */

QScrollBar:horizontal {
  height: 18px;
  border-right: 20px solid rgba(250,250,250,0.1);
  border-left: 20px solid rgba(250,250,250,0.1);
  border-top: 2px solid rgba(250,250,250,0.1);
  border-bottom: 2px solid rgba(250,250,250,0.1);
  background-color: transparent;
  margin-bottom: 2px;
  margin-top: 2px;
}

QScrollBar:vertical {
  width: 18px;
  border-top: 20px solid rgba(250,250,250,0.1);
  border-bottom: 20px solid rgba(250,250,250,0.1);
  border-left: 2px solid rgba(250,250,250,0.1);
  border-right: 2px solid rgba(250,250,250,0.1);
  background-color: transparent;
  margin-left: 2px;
  margin-right: 2px;
}

QScrollBar::handle:horizontal {
  background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 transparent,stop:0.5 rgba(0,0,0,0.2), stop:1 transparent);
}

QScrollBar::handle:horizontal:hover {
  background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 transparent,stop:0.5 @ThemeAccentColor1, stop:1 transparent);
}

QScrollBar::handle:horizontal:focus {
  border: 1px solid transparent;
}

QScrollBar::handle:vertical {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 transparent,stop:0.5 rgba(0,0,0,0.2), stop:1 transparent);
}

QScrollBar::handle:vertical:hover {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 transparent,stop:0.5 @ThemeAccentColor1, stop:1 transparent);
}

QScrollBar::handle:vertical:focus {
  border: 1px solid transparent;
}

QScrollBar::add-line:horizontal {
  margin: 2px 2px;
  image: url(qss:images_classic/Arrow-right-black.png);
  height: 14px;
  width: 14px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover, 
QScrollBar::add-line:horizontal:on {
  image: url(qss:images_classic/Arrow-right-white.png);
  height: 14px;
  width: 14px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::add-line:vertical {
  margin: 2px 2px;
  image: url(qss:images_classic/Arrow-down-black.png);
  height: 14px;
  width: 14px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on {
  image: url(qss:images_classic/Arrow-down-white.png);
  height: 14px;
  width: 14px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
  margin: 2px 2px;
  image: url(qss:images_classic/Arrow-left-black.png);
  height: 14px;
  width: 14px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover, 
QScrollBar::sub-line:horizontal:on {
  image: url(qss:images_classic/Arrow-left-white.png);
  height: 14px;
  width: 14px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
  margin: 2px 2px;
  image: url(qss:images_classic/Arrow-up-black.png);
  height: 14px;
  width: 14px;
  subcontrol-position: top;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover, 
QScrollBar::sub-line:vertical:on {
  image: url(qss:images_classic/Arrow-up-white.png);
  height: 14px;
  width: 14px;
  subcontrol-position: top;
  subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
  background-color: none;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
  background-color: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
  background-color: none;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
  background-color: none;
}

