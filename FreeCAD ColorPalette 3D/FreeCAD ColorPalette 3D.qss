/* QScrollBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qscrollbar

--------------------------------------------------------------------------- */

QScrollBar:horizontal {
  height: 18px;
  border-right: 20px solid rgba(250,250,250,0.1);
  border-left: 20px solid rgba(250,250,250,0.1);
  border-top: 2px solid rgba(250,250,250,0.1);
  border-bottom: 2px solid rgba(250,250,250,0.1);
  background-color: transparent;
  margin-bottom: 2px;
  margin-top: 2px;
}

QScrollBar:vertical {
  width: 18px;
  border-top: 20px solid rgba(250,250,250,0.1);
  border-bottom: 20px solid rgba(250,250,250,0.1);
  border-left: 2px solid rgba(250,250,250,0.1);
  border-right: 2px solid rgba(250,250,250,0.1);
  background-color: transparent;
  margin-left: 2px;
  margin-right: 2px;
}

QScrollBar::handle:horizontal {
  background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 transparent,stop:0.5 rgba(0,0,0,0.2), stop:1 transparent);
}

QScrollBar::handle:horizontal:hover {
  background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 transparent,stop:0.5 @ThemeAccentColor1, stop:1 transparent);
}

QScrollBar::handle:horizontal:focus {
  border: 1px solid transparent;
}

QScrollBar::handle:vertical {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 transparent,stop:0.5 rgba(0,0,0,0.2), stop:1 transparent);
}

QScrollBar::handle:vertical:hover {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 transparent,stop:0.5 @ThemeAccentColor1, stop:1 transparent);
}

QScrollBar::handle:vertical:focus {
  border: 1px solid transparent;
}

QScrollBar::add-line:horizontal {
  margin: 2px 2px;
  image: url(qss:images_classic/Arrow-right-black.png);
  height: 14px;
  width: 14px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover, 
