/* QMainWindow ------------------------------------------------------------

This adjusts the splitter in the dock widget, not qsplitter
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmainwindow

--------------------------------------------------------------------------- */
QMainWindow::separator {
  border-right: 0px solid rgba(0,0,0,0.2);
  spacing: 0px;
  padding: 0px;
}

QMainWindow::separator:vertical {
  image: url(qss:images_classic/splitter_vertical-black.png);
}

QMainWindow::separator:horizontal {
  image: url(qss:images_classic/splitter_horizontal-black.png);
}

QMainWindow::separator:vertical:hover {
  image: url(qss:images_classic/splitter_vertical-white.png);
}

QMainWindow::separator:horizontal:hover {
  image: url(qss:images_classic/splitter_horizontal-white.png);
}

QMainWindow::separator:horizontal {
  height: 4px;
  margin-top: 1px;
  margin-bottom: 1px;
}

QMainWindow::separator:vertical {  
  width: 4px;
  margin-left: 1px;
  margin-right: 1px;
}

/* QToolTip ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtooltip

--------------------------------------------------------------------------- */
QToolTip {
  background-color: #FFFFE1;
  color: black;
  /* If you remove the border property, background stops working on Windows */
  border: none;
  font-weight:bolder;
  border-top-width: 1px;
  border-left-width: 1px;
  border-right-width: 1px;
  border-bottom-width: 1px;
  border-style: solid;
  border-color: transparent;
  /* Remove padding, for fix combo box tooltip */
  padding: 0px;
  /* Remove opacity, fix #174 - may need to use RGBA */
  /* border-radius: 4px; */
}

/* QStatusBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qstatusbar

--------------------------------------------------------------------------- */
QStatusBar {
  border: 0px solid rgba(0,0,0,0.2);
  /* Fixes Spyder #9120, #9121 */
  background-color: transparent;
  /* Fixes #205, white vertical borders separating items */
}

QStatusBar::item {
  border: none;
}

QStatusBar QToolTip {
  background-color: #FFFFE1;
  border: 1px solid transparent;
  color: black;
  /* Remove padding, for fix combo box tooltip */
  padding: 0px;
  /* Reducing transparency to read better */
  opacity: 230;
}

QStatusBar QLabel {
  /* Fixes Spyder #9120, #9121 */
  background-color: transparent;
}

/* QCheckBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcheckbox

--------------------------------------------------------------------------- */
QCheckBox {
  color: #dddddd;
  padding: 0px;
  outline: none;
  border: 1px solid transparent;
  background-color: transparent;
  spacing: 8px;
}

QCheckBox:focus {
  border: 1px solid transparent;
  border-radius: 2px;
}

QCheckBox QWidget:disabled {
  color: #bbbbbb;
}

QCheckBox::indicator {
  color: #dddddd;
  background-color: rgba(0,0,0,0.7);
  width: 14px;
  height: 14px;
  border: 1px solid rgba(0,0,0,0.7);
  border-radius: 2px;
}

QCheckBox::indicator:unchecked {
  background-color: rgba(0,0,0,0.7);
  border: 1px solid rgba(0,0,0,0.7);
}

QCheckBox::indicator:unchecked:hover, 
QCheckBox::indicator:unchecked:focus, 
QCheckBox::indicator:unchecked:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid #bbbbbb;
}

QCheckBox::indicator:unchecked:disabled {
  background-color: #444444;
  border: 1px solid #696969;
}

QCheckBox::indicator:checked {
  image:url(qss:images_classic/check-mark-blue.png);
  border: 1px solid rgba(0,0,0,0.7);
  background-color: rgba(0,0,0,0.7);
}

QCheckBox::indicator:checked:hover, 
QCheckBox::indicator:checked:pressed {
  image:url(qss:images_classic/check-mark-white.png);
  background-color: @ThemeAccentColor1;
  border: 1px solid #bbbbbb;
}

QCheckBox::indicator:checked:disabled {
  background-color: #444444;
  border: 1px solid #696969;
  image: url(qss:images_classic/check-mark-lightgray.png);
}

QCheckBox::indicator:indeterminate {
  background-color: rgba(0,0,0,0.7);
  border: 1px solid rgba(0,0,0,0.7);
  image: url(qss:images_classic/transparent.png);
}

QCheckBox::indicator:indeterminate:disabled {
  background-color: #444444;
  border: 1px solid #696969;
  image: url(qss:images_classic/transparent.png);
}

QCheckBox::indicator:indeterminate:hover,
QCheckBox::indicator:indeterminate:pressed {
  background-color: @ThemeAccentColor1;
    border: 1px solid rgba(0,0,0,0.7);
  }

/* QGroupBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qgroupbox

--------------------------------------------------------------------------- */
QGroupBox {
  background-color: rgba(0,0,0,0.1);
  border: 1px solid rgba(0,0,0,0.2);
  border-radius: 10px;
  margin-top: 10px; 
  padding-top: 5px;
  font-weight: bold;
}

QGroupBox::title {
  top: -8px;
  left: 15px;
  color: @ThemeAccentColor1;
}

QGroupBox::indicator {
  background-color: rgba(0,0,0,0.7);
  width: 14px;
  height: 14px;
  border: 1px solid transparent;
  border-radius: 2px;
}

QGroupBox::indicator:unchecked {
  background-color: rgba(0,0,0,0.7);  
  border: 1px solid transparent;
}

QGroupBox::indicator:unchecked:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid #bbbbbb;
}

QGroupBox::indicator:unchecked:focus {
  border: 1px solid #bbbbbb;
}

QGroupBox::indicator:unchecked:disabled {
  background-color: #444444;
  border: 1px solid #696969;
}

QGroupBox::indicator:checked {
  background-color: rgba(0,0,0,0.7);
  border: 1px solid transparent;
  image:url(qss:images_classic/check-mark-blue.png);
}

QGroupBox::indicator:checked:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid #bbbbbb;
  image:url(qss:images_classic/check-mark-white.png);
}

QGroupBox::indicator:checked:focus {
  border: 1px solid #bbbbbb;
  image:url(qss:images_classic/check-mark-white.png);
}

QGroupBox::indicator:checked:disabled {
  background-color: #444444;
  image:url(qss:images_classic/check-mark-lightgray.png);
  border: 1px solid #bbbbbb;
}

/* QRadioButton -----------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qradiobutton

--------------------------------------------------------------------------- */
QRadioButton {
  background-color: transparent;
  color: #dddddd;
  border: none;
  spacing: 8px;
  /* padding-top: 4px;
  padding-bottom: 4px; */
  outline: none;
}

QRadioButton:focus {
  border: 0px solid @ThemeAccentColor1;
}

QRadioButton:disabled {
  background-color: #444444;
}

QRadioButton QWidget {
  background-color: transparent;
  color: #dddddd;
  spacing: 0px;
  padding: 0px;
  outline: none;
  border: none;
}

QRadioButton::indicator {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-unchecked-black.png);
}

QRadioButton::indicator:unchecked {
}

QRadioButton::indicator:unchecked:hover, QRadioButton::indicator:unchecked:pressed {

}

QRadioButton::indicator:unchecked:disabled {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-unchecked-lightgray.png);
}

QRadioButton::indicator:checked {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-unchecked-white.png);
}

QRadioButton::indicator:checked:hover, QRadioButton::indicator:checked:pressed {

}

QRadioButton::indicator:checked:disabled {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-checked-lightgray.png);
}

/* QMenuBar ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenubar

--------------------------------------------------------------------------- */
QMenuBar {
  background-color: transparent;
  color: #dddddd;
  selection-background-color: @ThemeAccentColor1;
}

QMenuBar:focus {
  border: 0px solid @ThemeAccentColor1;
}

QMenuBar::item {
  background-color: transparent;
  padding: 4px;
  margin: 0px;
}

QMenuBar::item:hover {
  border: 0px solid rgba(0,0,0,0.4);
  background-color: @ThemeAccentColor1;
}

QMenuBar::item:selected {
  border: 0px solid rgba(0,0,0,0.4);
  background-color: @ThemeAccentColor1;
}

QMenuBar::item:pressed {
  border: 0px solid rgba(0,0,0,0.4);
  background-color: @ThemeAccentColor1;
  color: white;
}

/* QMenu ------------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenu

--------------------------------------------------------------------------- */
QMenu {
  margin: 0px;
  padding: 4px;
  border: 1px solid rgba(0,0,0,0.4);
  border-radius: 0px;
  color: #dddddd;  
  background-color: @ThemeAccentColor3;
}

QMenu::separator {
  height: 1px;
  background-color: rgba(0,0,0,0.5);
  margin-left: 10%;
  margin-right: 10%;
  margin-top: 3px;
  margin-bottom: 3px;
}

QMenu::item {
  background-color: transparent;
  padding-left: 10px;
  padding-top: 3px;
  padding-bottom: 3px;
  padding-right: 5px;
  border: 1px rgba(0,0,0,0.4);
  border-radius: 4px;
}

QMenu::item:selected {
  color: white;
  background-color: @ThemeAccentColor1;
  border: 1px rgba(0,0,0,0.4);
}

QMenu::item:hover {
  color: white;
  background-color: @ThemeAccentColor1;
  border: 1px rgba(0,0,0,0.4);
}

QMenu::item:pressed {
  color: white;
  background-color: @ThemeAccentColor1;
  border: 1px rgba(0,0,0,0.4);
}

QMenu::item:disabled {
  color: #bbbbbb;
}

QMenu::icon {
  margin-left: 10px;
  width: 14px;
  height: 14px;
}

QMenu::indicator {
  background-color: rgba(0,0,0,0.7);
  margin-left: 4px;
  margin-right: 4px;
  width: 14px;
  height: 14px;
  border: 1px solid rgba(0,0,0,0.7);
  border-radius: 2px;
  /* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
  /* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
}

QMenu::icon:checked { /* appearance of a 'checked' icon */
  margin-left: 4px;
  margin-right: 4px;
  background-color: rgba(0,0,0,0.9);
  border: 3px solid rgba(0,0,0,0.9);
  position: absolute;
  border-radius: 2px;
}
QMenu::indicator:non-exclusive:unchecked {
  background-color: rgba(0,0,0,0.7);
}

QMenu::indicator:non-exclusive:unchecked:hover, 
QMenu::indicator:non-exclusive:unchecked:focus, 
QMenu::indicator:non-exclusive:unchecked:pressed {
  border: 1px solid #bbbbbb;
  background-color: @ThemeAccentColor1;
}

QMenu::indicator:non-exclusive:unchecked:disabled {
  image: url(qss:images_classic/checkbox-Empty-lightgray.png);
}

QMenu::indicator:non-exclusive:checked {
  image:url(qss:images_classic/check-mark-blue.png);
}

QMenu::indicator:non-exclusive:checked:hover,
QMenu::indicator:non-exclusive:checked:focus,
QMenu::indicator:non-exclusive:checked:pressed {
  border: 1px solid #bbbbbb;
  background-color: @ThemeAccentColor1;
  image:url(qss:images_classic/check-mark-white.png);
}

QMenu::indicator:non-exclusive:checked:disabled {
image:url(qss:images_classic/checkbox-checked-lightgray.png);
}

QMenu::indicator:non-exclusive:indeterminate {
  image: url(qss:images_classic/checkbox-indeterminate-white.png);
}

QMenu::indicator:non-exclusive:indeterminate:disabled {
  image: url(qss:images_classic/checkbox-indeterminate-white.png);
}

QMenu::indicator:non-exclusive:indeterminate:focus,
QMenu::indicator:non-exclusive:indeterminate:hover,
QMenu::indicator:non-exclusive:indeterminate:pressed {
  image: url(qss:images_classic/checkbox-indeterminate-white.png);
}

QMenu::indicator:exclusive:unchecked {
  image: url(qss:images_classic/transparent.png);
}

QMenu::indicator:exclusive:unchecked:hover,
QMenu::indicator:exclusive:unchecked:focus,
QMenu::indicator:exclusive:unchecked:pressed {
  border: none;
  outline: none;
  background-color: @ThemeAccentColor1;
  image: url(qss:images_classic/checkbox-Empty-white.png);
}

QMenu::indicator:exclusive:unchecked:disabled {
  image: url(qss:images_classic/checkbox-Empty-white.png);
}

QMenu::indicator:exclusive:checked {
  border: 1px solid rgba(0,0,0,0.7);
  outline: none;
  image: url(qss:images_classic/check-mark-blue.png);
}

QMenu::indicator:exclusive:checked:hover,
QMenu::indicator:exclusive:checked:focus,
QMenu::indicator:exclusive:checked:pressed {
  border: none;
  outline: none;
  background-color: @ThemeAccentColor1;
  image: url(qss:images_classic/checkbox-Empty-white.png);
}

QMenu::indicator:exclusive:checked:disabled {
  outline: none;
  image: url(qss:images_classic/Radio-checked-white.png);
}

QMenu::right-arrow {
  margin: 5px;
  padding-left: 12px;
  image:url(qss:images_classic/Arrow-right-white.png);
  height: 10px;
  width: 10px;
  background-color: transparent;
}

/* QAbstractItemView ------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcombobox

--------------------------------------------------------------------------- */
QAbstractItemView {
  alternate-background-color: rgba(0,0,0,0.2);
