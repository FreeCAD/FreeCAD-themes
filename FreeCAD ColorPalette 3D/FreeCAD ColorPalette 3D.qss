---------------------------------------------------------------------------*/
QSplitterHandle:hover { /* to fix bug about hovering on splitters https://bugreports.qt.io/browse/QTBUG-13768 */

}

QSplitter::handle {
  margin: 0px 0px;
  padding: 0px;
}

QSplitter::handle:horizontal {
  background-image: none;
  background-position: center center;
  background-repeat: none;
  width: 4px;
  margin-top: 0px;
  margin-bottom: 0px;
}

QSplitter::handle:vertical {
  background-image: none;
  background-position: center center;
  background-repeat: none;
  height: 4px;
  margin-left: 0px;
  margin-right: 0px;
}

QSplitter::handle:vertical {
  image: url(qss:images_classic/splitter_horizontal-black.png);
}

QSplitter::handle:horizontal {
  image: url(qss:images_classic/splitter_vertical-black.png);
}

